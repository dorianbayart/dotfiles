{
  "git_panel": {
    "dock": "right"
  },
  "disable_ai": false,
  "show_edit_predictions": false,
  "vim_mode": false,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "theme": "Tokyo Night",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "format_on_save": "off",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "Zed Mono",
  "languages": {
    "PHP": {
      "language_servers": ["!intelephense", "phpactor", "..."]
    },
    "Go": {
      "format_on_save": "language_server"
    }
  }
}
